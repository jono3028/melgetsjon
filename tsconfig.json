{
	"compilerOptions": {
		"outDir": "./dist",
		"sourceMap": true,
		"noImplicitAny": true,
		"module": "commonjs",
		"target": "es5",
		"lib": ["es6","dom"],
		"jsx": "react",
		"baseUrl": ".",
		"paths": {
				"~/*": ["src/*"]
		},
	},
	"include": [
		"./src/**/*"
	],
	"exclude": [
		"node_modules"
	]
}
